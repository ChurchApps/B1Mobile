{
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true,
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.printWidth": 0,
  "editor.tabSize": 2,
  "html.format.wrapLineLength": 0
}
