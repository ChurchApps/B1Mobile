{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "amazon": {
      "autoIncrement": false,
      "environment": "production",
      "android": {
        "distribution": "internal",
        "buildType": "apk"
      },
      "channel": "amazon"
    }
  },
  "submit": {
    "GooglePlay": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json"
      }
    },
    "Amazon": {
      "android": {}
    }
  }
}
