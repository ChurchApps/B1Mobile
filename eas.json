{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "env": {
          "GOOGLE_SERVICES_JSON": "1"
        }
      }
    }
  },
  "submit": {
    "GooglePlay": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json"
      }
    }
  }
}